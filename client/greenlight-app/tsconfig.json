{
  "compilerOptions": {
    "jsx": "react-native",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "view/*": [
        "view/*"
      ],
      "svg/*": [
        "components/svg/*"
      ],
      "domain/*": [
        "domain/*"
      ],
      "service/*": [
        "service/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "controller/*": [
        "controller/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "interfaces/*": [
        "domain/interfaces/*"
      ],
      "types/*": [
        "domain/types/*"
      ],
      "config/*": [
        "config/*"
      ],
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@react-native-async-storage/async-storage"
    ]
  },
  "exclude": [
    "node_modules",
    // "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}