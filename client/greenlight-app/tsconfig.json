{
  "compilerOptions": {
    "jsx": "react-native",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "view/*": [
        "view/*"
      ],
      "svg/*": [
        "components/svg/*"
      ],
      "domain/*": [
        "domain/*"
      ],
      "service/*": [
        "components/service/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "controller/*": [
        "controller/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "interfaces/*": [
        "domain/interfaces/*"
      ],
      "types/*": [
        "domain/types/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    // "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}